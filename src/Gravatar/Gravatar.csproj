<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageId>LucasRosinelli.Gravatar</PackageId>
    <Authors>Lucas Rosinelli</Authors>
    <Product>Extensions for Gravatar</Product>
    <Description>Extensions for Gravatar</Description>
    <Title>Gravatar Extensions</Title>
    <RepositoryUrl>https://github.com/LucasRosinelli/dotnet-gravatar-package</RepositoryUrl>
    <PackageProjectUrl>https://github.com/LucasRosinelli/dotnet-gravatar-package</PackageProjectUrl>
    <PackageTags>Gravatar</PackageTags>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
